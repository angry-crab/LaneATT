{
  "containerEnv": {
      "QT_X11_NO_MITSHM": "1",
      "DISPLAY": "${localEnv:DISPLAY}",
  },
  "remoteEnv": {
      "DOCKER_BUILDKIT": "0",
  },
  "image": "ghcr.io/autowarefoundation/autoware-universe:humble-latest-cuda",
  "extensions": [
      "ms-vscode.cpptools",
      "ms-vscode.cmake-tools",
      "twxs.cmake",
      "xaver.clang-format",
      "jeff-hykin.better-cpp-syntax",
      "mhutchie.git-graph",
      "cschlosser.doxdocgen",
      "wayou.vscode-todo-highlight",
      "ms-iot.vscode-ros",
      "ms-python.python",
      "yzhang.markdown-all-in-one",
      "njpwerner.autodocstring",
      "davidanson.vscode-markdownlint",
      "bierner.markdown-mermaid",
      "visualstudioexptteam.vscodeintellicode",
      "streetsidesoftware.code-spell-checker",
      "eamodio.gitlens",
      "ryu1kn.partial-diff",
      "formulahendry.code-runner",
      "pkief.material-icon-theme",
      "alefragnani.bookmarks",
      "ms-vscode.cpptools-extension-pack",
      "matepek.vscode-catch2-test-adapter",
      "geeebe.duplicate",
      "mrmlnc.vscode-duplicate"
  ],
  "workspaceMount": "source=/home/xinyuwang/adehome/LaneATT,target=/home/LaneATT,type=bind",
  "workspaceFolder": "/home/LaneATT",
  "runArgs": [
      "--gpus",
      "all",
      "--volume=/tmp/.X11-unix:/tmp/.X11-unix",
      "--name=autoware_vscode_test",
      "--privileged",
      "--pid=host",
      "--volume=/dev:/dev",
      "--volume=/tmp:/tmp",
  ]
}
